<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns   = "http://www.w3.org/ns/widgets"
    xmlns:gap   = "http://phonegap.com/ns/1.0"
    id          = "com.entregapp.novo"
    version     = "1.0.4" >
	<name>Helio Equipamentos</name>
        <description>
          Consulte o nosso catálogo de produtos de uma forma rápida e prática utilizando o aplicativo Helio Equipamentos by Entregapp.
        </description>
        <author email="contato@entregapp.com.br" href="http://entregapp.com.br">
            Eduardo Alves
        </author>
         <feature name="debug-server" required="false">
            <param name="domain" value="http://localhost/0003P"/>
            <param name="key" value="com.entregapp.novo"/>
          </feature>
          <plugin name="io.litehelpers.cordova.sqlite" spec="0.7.10" source="pgb" />

        <gap:plugin name="nl.x-services.plugins.socialsharing" version="4.3.8" />
        <plugin name="com.indigoway.cordova.whitelist.whitelistplugin" spec="1.1.1" source="pgb" />
        <preference name="Fullscreen" value="false" />
        <preference name="WebViewBounce" value="true" />
        <feature name="http://api.phonegap.com/1.0/geolocation"/>
        <icon src="icon.png" />
        <gap:splash src="splash.png" />
</widget>
